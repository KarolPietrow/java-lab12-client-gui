<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.lab12clientgui.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

  <TextArea fx:id="textArea" focusTraversable="false" prefHeight="200.0" prefWidth="200.0" />
  <TextField fx:id="textField" onAction="#onButtonClick" />
  <Button onAction="#onButtonClick" text ="Button" />

</VBox>
